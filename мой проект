REACT
yarn add react                                                                                  -   установка react
yarn add -D @types/react                                                                        -   установка типов react

yarn add react-dom                                                                              -   установка react-dom
yarn add -D @types/react-dom                                                                    -   установка типов react-dom

yarn add -D @testing-library/jest-dom                                                           -   установка testing-library/jest-dom
yarn add -D @testing-library/react                                                              -   установка testing-library/react
yarn add -D @testing-library/user-event                                                         -   установка testing-library/user-event

yarn add -D @types/jest                                                                         -   установка типов types/jest
yarn add -D @types/node                                                                         -   установка типов types/node

TYPESCRIPT

yarn add -D typescript                                                                          -   установка typescript
yarn add -D @types/react                                                                        -   установка типов react
yarn add -D @types/react-dom                                                                    -   установка типов react-dom
yarn add -D @testing-library/jest-dom                                                           -   установка типов testing-library/jest-dom
yarn add -D @testing-library/react                                                              -   установка типов testing-library/react
yarn add -D @testing-library/user-event                                                         -   установка типов testing-library/user-event
yarn add -D @types/jest                                                                         -   установка типов types/jest
yarn add -D @types/node                                                                         -   установка типов types/node
yarn add node-sass --dev																		-	установка sass
yarn add @types/node-sass --dev																	-	установка sass типы для typescript

WEBPACK
yarn add webpack --dev																			-	установка webpack корневой функционал --dev только для разработки
yarn add @types/webpack --dev                                                                   -	добавление типов
yarn add -D webpack-merge                                                                       -   позволяет объединять файлы
yarn add webpack-cli --dev																		-	установка webpack-cli обеспечивает доступ работы с консолью используя команды webpack
yarn add html-webpack-plugin --dev 																-	плагин позволяет взаимодействовать с html (подключает в html файл все скрипты указанные в output)
yarn add clean-webpack-plugin --dev																-	удаляет устаревшие файл построенные webpack'ом (например хэшированные)
yarn add style-loader css-loader --dev															-	устанавливаем плагин позволяющий webpack'у работать с css
yarn add webpack-dev-server --dev																-	устанавливаем плагин позволяющий с билдом на сервере который хранит все файлы в оперативной памяти
yarn add @types/webpack-dev-server --dev														-	добавление типов
yarn add copy-webpack-plugin --dev																-	устанавливаем плагин позволяющий webpack'у переносить все необходимы файлы (картинки) переносить в билд
yarn add sass-loader --dev																		-	устанавливаем пакета для sass
npm install --save-dev cross-env																-	устанавливаем пакета для установки своих переменных
npm install --save-dev @babel/polyfill															-	имитация полной среды ES2015+
yarn add --dev tsconfig-paths-webpack-plugin													-	плагин для получения путей из файла tsconfig.json
yarn add -D typescript-plugin-css-modules                                                       -   плагин для загрузки css в файлах typescript
yarn add -D @pmmmwh/react-refresh-webpack-plugin react-refresh                                  -   плагин позволяет не перезагружать страницу после внесения правок в коде
yarn add --dev fork-ts-checker-webpack-plugin                                                   -   Плагин Webpack, который запускает проверку типов TypeScript в отдельном процессе.

BABLE
npm install --save-dev babel-loader @babel/core 												-	bable позволяет компилировать новый js код для устаревших браузеров
npm install --save-dev @babel/preset-react														-	пресет для react
npm install --save-dev @babel/preset-typescript													-	пресет для typescript
yarn add @babel/preset-env --dev																-	пресет для babel
yarn add @babel/plugin-transform-runtime --dev                                                  -   Вынесите ссылки на помощники и встроенные функции, автоматически заполнив ваш код, не загрязняя глобальные объекты.

LIBRARY
npm install --save react-router-dom																-	плагин для работы с маршрутами
npm install --save-dev @types/react-router-dom													-	типы для typescript
npm install redux --save																		-	установка redux
npm install --save-dev @types/redux																-	типы для typescript
npm install react-redux --save																	-	установка react-redux это прослойка между интерфейсом и бизнес логикой
npm install --save-dev @types/react-redux														-	типы для typescript
npm install redux-thunk	--save																	-	установка thunk
npm install --save-dev @types/redux-thunk														-	типы для typescript
npm install axios --save																		-	установка библиотеки axios для выполнения ajax запросов
npm install --save-dev @types/axios																-	типы для typescript
yarn add formik                                                                                 -   работа с формами
yarn add use-react-router-breadcrumbs                                                           -   хлебные крошки
npm install react-icons --save                                                                  -   иконки для react
npm install react-transition-group --save                                                       -   анимация
npm install normalize.css --save                                                                -   normalize
npm install react-localization --save                                                           -   локализация
npm install react-draft-wysiwyg --save                                                          -   редактор
npm install @types/react-draft-wysiwyg --save-dev


npm install html-to-draftjs --save                                                              -   конвертация результат HTML для редактора
npm install @types/html-to-draftjs --save-dev

npm install draftjs-to-html --save                                                              -   конвертация результат редактора в HTML
npm install @types/draftjs-to-html --save-dev




npm install uuid --save                                                                         -   uuid
npm install react-paginate --save                                                               -   paginate
npm install --save-dev @types/react-paginate                                                    -   paginate type

DEBUG
npm install --save-dev redux-devtools-extension													-	дополнение для браузера, для разработки
npm install --save-dev @types/redux-devtools-extension											-	типы для typescript


ESLINT
npm install eslint --save-dev
npm install eslint-import-resolver-typescript --save-dev
npm install eslint-plugin-react-hooks --save-dev
npm install eslint-plugin-sort-imports-es6-autofix --save-dev


npm install patch-package --save-dev                                                            -   для изменений в node_model
npm install gh-pages --save-dev                                                                 -   для публикаций gh-pages

